{
	"name": "cli-pago_mis_servicios",
	"version": "0.0.0",
	"type": "module",
	"description": "Modern Node.js Command Line Tool to pay bills automatically.",
	"author": "Daniel Patricio Becerra",
	"repository": {
		"directory": "dev",
		"url": "https://github.com/dapabe/CLI-Pago_mis_servicios"
	},
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"prepare": "husky",
		"prepublishOnly": "npm run build",
		"build:dev": "bun --env-file=.env.development --ignore-scripts scripts/bundler.mjs",
		"build:prod": "bun --env-file=.env.production --ignore-scripts scripts/bundler.mjs",
		"gen:exe": "bun build --compile --minify --target=bun-windows-x64-modern dist/index.js --outfile dist/targets",
		"start": "bun --env-file=.env.production dist/index.cjs",
		"serve": "bun --env-file=.env.development dist/index.cjs",
		"commitSemver": "changeset",
		"addToChangelog": "changeset version"
	},
	"dependencies": {
		"@clack/prompts": "0.7.0",
		"@msgpack/msgpack": "3.0.0-beta2",
		"crypto-js": "4.2.0",
		"electron": "31.3.1",
		"is-online": "10.0.0",
		"node-cleanup": "^2.1.2",
		"picocolors": "1.0.1",
		"playwright-core": "1.45.3",
		"semver": "7.6.2",
		"update-notifier": "^7.0.0",
		"zod": "3.23.8",
		"zod-defaults": "0.1.3",
		"zod-semver": "1.3.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@changesets/cli": "^2.27.6",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/bun": "latest",
		"@types/crypto-js": "4.2.2",
		"@types/node-cleanup": "2.1.5",
		"@types/semver": "7.5.8",
		"@types/update-notifier": "6.0.8",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"typescript": "5.5.4"
	},
	"trustedDependencies": ["playwright-core"]
}
